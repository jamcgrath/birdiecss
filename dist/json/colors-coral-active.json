{
  ".coral-light_active:active": {
    "color": "var(--coral-light)"
  },
  ".coral_active:active": {
    "color": "var(--coral)"
  },
  ".coral-dark_active:active": {
    "color": "var(--coral-dark)"
  },
  ".coral-light-bg_active:active": {
    "backgroundColor": "var(--coral-light)"
  },
  ".coral-bg_active:active": {
    "backgroundColor": "var(--coral)"
  },
  ".coral-dark-bg_active:active": {
    "backgroundColor": "var(--coral-dark)"
  },
  "@media (min-width: 480px)": true,
  "@media (min-width: 768px)": true,
  "@media (min-width: 1024px)": true,
  "@media (min-width: 1440px)": true,
  "@media (min-width: 1920px)": true
}