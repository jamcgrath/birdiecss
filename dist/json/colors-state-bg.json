{
  ".navy-hover-bg:hover": {
    "backgroundColor": "var(--hover-navy)"
  },
  ".navy-light-hover-bg:hover": {
    "backgroundColor": "var(--hover-navy-light)"
  },
  ".grape-hover-bg:hover": {
    "backgroundColor": "var(--hover-grape)"
  },
  ".grape-light-hover-bg:hover": {
    "backgroundColor": "var(--hover-grape-light)"
  },
  ".error-hover-bg:hover": {
    "backgroundColor": "var(--hover-error)"
  },
  ".navy-pressed-bg:active": {
    "backgroundColor": "var(--pressed-navy)"
  },
  ".navy-light-pressed-bg:active": {
    "backgroundColor": "var(--pressed-navy-light)"
  },
  ".grape-light-pressed-bg:active": {
    "backgroundColor": "var(--pressed-grape-light)"
  },
  ".error-pressed-bg:active": {
    "backgroundColor": "var(--pressed-error)"
  },
  "@media (min-width: 480px)": true,
  "@media (min-width: 768px)": true,
  "@media (min-width: 1024px)": true,
  "@media (min-width: 1440px)": true,
  "@media (min-width: 1920px)": true
}