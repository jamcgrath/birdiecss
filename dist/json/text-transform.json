{
  ".text-transform-none": {
    "textTransform": "none"
  },
  ".capitalize": {
    "textTransform": "capitalize"
  },
  ".uppercase": {
    "textTransform": "uppercase"
  },
  ".lowercase": {
    "textTransform": "lowercase"
  },
  "@media (min-width: 480px)": true,
  "@media (min-width: 768px)": true,
  "@media (min-width: 1024px)": true,
  "@media (min-width: 1440px)": true,
  "@media min-width: 1920px)": true
}