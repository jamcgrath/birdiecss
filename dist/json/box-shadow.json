{
  ".box-shadow-sm,\n.box-shadow-med,\n.box-shadow-lg": {
    "--bs-x": "0",
    "--bs-y": "4px",
    "--bs-blur": "12px",
    "--bs-spread": "0",
    "--bs-color": "rgba(0, 0, 0, 0.04)",
    "boxShadow": "var(--bs-x) var(--bs-y) var(--bs-blur) var(--bs-spread)\n    var(--bs-color)"
  },
  ".box-shadow-med": {
    "--bs-y": "6px",
    "--bs-color": "rgba(0, 0, 0, 0.12)"
  },
  ".box-shadow-lg": {
    "--bs-y": "12px",
    "--bs-blur": "24px",
    "--bs-color": "rgba(0, 0, 0, 0.24)"
  },
  "@media (min-width: 480px)": true,
  "@media (min-width: 768px)": true,
  "@media (min-width: 1024px)": true,
  "@media (min-width: 1440px)": true,
  "@media (min-width: 1920px)": true
}