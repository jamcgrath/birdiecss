{
  "._td,\n.td-underline,\n.td-overline,\n.td-strike,\n.td-underover,\n.td-wavy,\n.td-dot,\n.td-dash,\n.td-double,\n.td-underline_hover": {
    "--_td-color": "var(--td-color, currentcolor)",
    "--_td-thickness": "var(--td-thickness, auto)",
    "--_td-line": "var(--td-line, none)",
    "--_td-style": "var(--td-style, solid)",
    "WebkitTextDecorationColor": "var(--_td-color)",
    "textDecorationColor": "var(--_td-color)",
    "textDecorationThickness": "var(--_td-thickness)",
    "WebkitTextDecorationLine": "var(--_td-line)",
    "textDecorationLine": "var(--_td-line)",
    "WebkitTextDecorationStyle": "var(--_td-style)",
    "textDecorationStyle": "var(--_td-style)"
  },
  ".td-underline,\n.td-underline_hover:hover": {
    "--td-line": "underline"
  },
  ".td-overline": {
    "--td-line": "overline"
  },
  ".td-strike": {
    "--td-line": "line-through"
  },
  ".td-underover": {
    "--td-line": "underline overline"
  },
  ".td-wavy": {
    "--td-style": "wavy"
  },
  ".td-dot": {
    "--td-style": "dotted"
  },
  ".td-dash": {
    "--td-style": "dashed"
  },
  ".td-double": {
    "--td-style": "double"
  },
  "@media (min-width: 480px)": true,
  "@media (min-width: 768px)": true,
  "@media (min-width: 1024px)": true,
  "@media (min-width: 1440px)": true,
  "@media min-width: 1920px)": true
}