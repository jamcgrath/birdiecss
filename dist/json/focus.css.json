{
  ":root": {
    "--focus-outline-color": "var(--hover)",
    "--focus-outline-style": "solid",
    "--focus-outline-width": "1px",
    "--focus-outline-offset": "1px"
  },
  "*:focus,\n.focus": {
    "--_focus-color": "var(--focus-color, var(--focus-outline-color))",
    "--_focus-style": "var(--focus-style, var(--focus-outline-style))",
    "--_focus-width": "var(--focus-width, var(--focus-outline-width))",
    "--_focus-offset": "var(--focus-offset, var(--focus-outline-offset))",
    "outline": "var(--focus-width) var(--focus-style) var(--focus-color)",
    "outlineOffset": "var(--focus-offset)"
  },
  "@supports selector(:focus-visible)": {
    "*:focus": {
      "outline": "0"
    },
    "*.focus-visible": {
      "--_focus-color": "var(--focus-color, var(--focus-outline-color))",
      "--_focus-style": "var(--focus-style, var(--focus-outline-style))",
      "--_focus-width": "var(--focus-width, var(--focus-outline-width))",
      "--_focus-offset": "var(--focus-offset, var(--focus-outline-offset))",
      "outline": "var(--focus-width) var(--focus-style) var(--focus-color)",
      "outlineOffset": "var(--focus-offset)"
    },
    "*:focus-visible": {
      "--_focus-color": "var(--focus-color, var(--focus-outline-color))",
      "--_focus-style": "var(--focus-style, var(--focus-outline-style))",
      "--_focus-width": "var(--focus-width, var(--focus-outline-width))",
      "--_focus-offset": "var(--focus-offset, var(--focus-outline-offset))",
      "outline": "var(--focus-width) var(--focus-style) var(--focus-color)",
      "outlineOffset": "var(--focus-offset)"
    }
  },
  "@media (--sm)": true,
  "@media (--md)": true,
  "@media (--lg)": true,
  "@media (--xl)": true,
  "@media (--xxl)": true
}