    "color": "var(--aqua-light)"
  },
  ".aqua_active:active": {
    "color": "var(--aqua)"
  },
  ".aqua-dark_active:active": {
    "color": "var(--aqua-dark)"
  },
  ".aqua-light-bg_active:active": {
    "backgroundColor": "var(--aqua-light)"
  },
  ".aqua-bg_active:active": {
