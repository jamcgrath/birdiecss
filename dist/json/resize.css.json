{
  ".resize-none": {
    "resize": "none"
  },
  ".resize-x": {
    "resize": "horizontal"
  },
  ".resize-y": {
    "resize": "vertical"
  },
  ".resize-both": {
    "resize": "both"
  },
  "@media (min-width: 300px)": true,
  "@media (min-width: 768px)": true,
  "@media (min-width: 1024px)": true,
  "@media (min-width: 1440px)": true,
  "@media (min-width: 1920px)": true
}